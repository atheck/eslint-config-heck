{
	"$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
	"extends": [
		"./biomeLinting.json"
	],
	"linter": {
		"rules": {
			"style": {
				"useNamingConvention": "off"
			},
			"suspicious": {
				"noRedundantUseStrict": "off"
			},
			"nursery": {
				"noCommonJs": "off"
			}
		}
	}
}