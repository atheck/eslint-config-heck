    "eslint": "8.10.0",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-react": "7.29.2",
    "eslint-plugin-typescript-heck": "1.2.0",
    "eslint-plugin-unicorn": "41.0.0",
    "typescript": "4.5.5"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
