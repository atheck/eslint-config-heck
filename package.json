{
  "name": "eslint-config-heck",
  "version": "1.5.0",
  "description": "Contains an ESLint configuration for ES2015+, TypeScript, and React.",
  "type": "module",
  "main": "index.js",
  "exports": {
    "./biomeJsTs": "./biomeJsTs.json",
    "./biomeCss": "./biomeCss.json",
    "./biomeJson": "./biomeJson.json",
    "./node": "./node.js",
    "./nodeWithBiome": "./nodeWithBiome.js",
    "./complexity": "./complexity.js",
    "./groupImports": "./groupImports.js",
    "./reactNative": "./reactNative.js"
  },
  "scripts": {
    "update": "npx -y npm-check-updates -i --install never && npx -y npm-check-updates -i --target minor --install never && npx -y npm-check-updates -i --target patch --install never && npm update --force",
    "lint": "eslint .",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "ESLint",
    "ES2015",
    "TypeScript",
    "React"
  ],
  "author": "atheck",
  "license": "MIT",
  "dependencies": {
    "@stylistic/eslint-plugin": "5.6.1",
    "@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "8.51.0",
    "eslint": "9.39.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.12.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-native": "5.0.0",
    "eslint-plugin-testing-library": "7.15.4",
    "eslint-plugin-unicorn": "62.0.0",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.1",
    "@types/jest": "30.0.0",
    "@types/react": "19.2.7",
    "globals": "17.0.0",
    "jest": "30.2.0",
    "react": "19.2.3",
    "semantic-release": "25.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atheck/eslint-config-heck.git"
  }
}
