{
	"$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
	"root": false,
	"linter": {
		"enabled": true,
		"rules": {
			"nursery": {
				"noDuplicateDependencies": "error"
			},
			"suspicious": {
				"noBiomeFirstException": "error",
				"noDuplicateObjectKeys": "error",
				"noQuickfixBiome": "error",
				"useBiomeIgnoreFolder": "error"
			}
		}
	}
}